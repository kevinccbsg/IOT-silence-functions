{
  "name": "IOT-silence-functions",
  "version": "1.0.0",
  "description": "Some functions with basic logic for silence standup bot",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "supervisor .",
    "test": "SERVICE_ENV=test mocha test --exit",
    "lint": "eslint .",
    "qa": "npm run lint && npm run test",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "docker": "bash -c '(docker network inspect local 2>&1 > /dev/null || docker network create local) && docker-compose --file docker/docker-compose.yml up -d --force-recreate'",
    "postinstall": "node_modules/.bin/eslint . --fix"
  },
  "devDependencies": {
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^7.5.1",
    "expect.js": "^0.3.1",
    "husky": "^0.14.3",
    "mocha": "^4.0.1",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "amqplib": "^0.5.3",
    "axios": "^0.19.0",
    "body-parser": "^1.15.2",
    "boom": "^3.2.2",
    "bunyan": "^1.8.5",
    "chalk": "^1.1.3",
    "confabulous": "^1.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "^2.2.0",
    "dotenv": "^8.0.0",
    "express-jsdoc-swagger": "^1.0.1",
    "hogan.js": "^3.0.2",
    "http-status-codes": "^1.3.2",
    "make-manifest": "^1.0.1",
    "mongodb": "^3.2.7",
    "on-headers": "^1.0.1",
    "optimist": "^0.6.1",
    "optional": "^0.1.3",
    "prepper": "^1.1.0",
    "ramda": "0.25.0",
    "systemic": "^3.0.0",
    "systemic-domain-runner": "^1.1.0",
    "systemic-express": "^1.1.0"
  }
}
